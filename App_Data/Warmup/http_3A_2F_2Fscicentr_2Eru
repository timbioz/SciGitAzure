									<section class="block block-views block-front-slider-flexslider block-views-front-slider-flexslider odd" id="block-views-front-slider-flexslider">
  <div class="block-inner clearfix">
              <h2 class='block-title clearfix'><span class='clearfix'>Flex Slider</span></h2>
            
